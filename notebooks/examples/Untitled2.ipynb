    "import multiprocessing as mp\n",
    " \n",
    "def my_func():\n",
    "  print(2**2)\n",
    " \n",
    "def main():\n",
    "  pool = mp.Pool(mp.cpu_count())\n",
    "  pool.map(my_func,())\n",
    " \n",
    "if __name__ == \"__main__\":\n",
    "  main()"
   ]
